{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "image/{imageId}"
    },
    {
      "name": "image",
      "type": "blob",
      "dataType": "binary",
      "path": "images/{imageId}.png",
      "connection": "IMAGE_STORAGE_CONNECTION",
      "direction": "in"
    },    
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "saveImage",
      "type": "blob",
      "dataType": "binary",
      "path": "images/{imageId}.png",
      "connection": "IMAGE_STORAGE_CONNECTION",
      "direction": "out"
    }    
  ],
  "scriptFile": "../dist/Image/ImageApi.js"
}